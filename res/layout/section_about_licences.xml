<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <TextView
        style="@style/CardAboutTitle"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/about_licences_title" />

    <LinearLayout style="@style/CardAbout" >

        <TextView
            android:id="@id/aboutLicenceIcons"
            style="@style/SectionContentItem"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:linksClickable="true"
            android:text="@string/about_licences_icons" />

        <View style="@style/Divider" />

        <TextView
            android:id="@id/aboutLicenceLibs"
            style="@style/SectionContentItem"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:linksClickable="true"
            android:text="@string/about_licences_libs" />
    </LinearLayout>

</merge>